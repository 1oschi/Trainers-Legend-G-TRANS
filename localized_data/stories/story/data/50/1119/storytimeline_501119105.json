{
  "Title": "夏合宿（3年目）スタート！",
  "TextBlockList": [
    {
      "Name": "",
      "Text": "",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "",
      "Text": "夏合宿が目前に迫る、ある日のこと――",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "…………――",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "……熱心だな。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "ああ、ファンレターかい？\n合宿所に持っていくわけにもいかないしね――",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "学園にいるうちに、\nなるべく返事を書いておきたいのさ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "…………『宝塚記念』。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "あの日の姉上は、明確に違った。\nそして、今もだ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "……たどり着いたのか？",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "……確かめているところさ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "『有馬記念』、楽しみにしていておくれ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "お前の期待もまた、私にとって、\n必要となるものなのだから。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "",
      "Text": "…………夏が来る――",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "",
      "Text": "蕩然たる青空に、雲を立てて――",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    }
  ]
}