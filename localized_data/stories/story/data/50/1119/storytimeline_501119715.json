    {
      "Name": "ドリームジャーニー",
      "Text": "本日のトレーニングは終了ですね。\nお疲れ様でし――",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "おや、あれは……。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "……オル。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "まだ走るのかい？",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "……ああ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "併走しようか。今日はずっと\n1人で走っていただろう？",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "見ていたのか？",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "お前はどこにいても目を引くからね。\nそれで、ともに走っても？",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "ああ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "ふふ、光栄だよ。オル。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "オルと併走してきます。\nよろしいですよね、トレーナーさん。",
      "ChoiceDataList": [
        "わかった。無理はしないように",
        "オーバーワークになる。許可できないよ"
      ],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "ご理解いただき、ありがとうございます。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "ご忠告には重々気をつけます。オルが\nオーバーワークになってはいけませんから。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "じゃあ、行こうか。オル。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "オルフェーヴル",
      "Text": "だそうだ。\n……またの機会に、姉上。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "ふ……賢明なご判断ですね。\nトレーナーさん？",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "ドリームジャーニー",
      "Text": "ですが、オルにああ言われてしまってはね。\n次の機会を待つことにいたしますよ。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    }
  ]
